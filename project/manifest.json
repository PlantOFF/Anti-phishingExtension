{
  "name": "URL Checker",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "main.js"
  },
  "action": {
    "default_popup": "html/popup.html"
  }
}
